{
  "name": "jongleur",
  "description": "An react-three-fiber orchestration library",
  "type": "module",
  "version": "0.2.6",
  "packageManager": "pnpm@7.12.0",
  "author": "Luca Fanselau <https://github.com/lucafanselau>",
  "license": "MIT",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./orchestrate": {
      "types": "./dist/orchestrate.d.ts",
      "require": "./dist/orchestrate.cjs",
      "import": "./dist/orchestrate.js"
    },
    "./progress": {
      "types": "./dist/progress.d.ts",
      "require": "./dist/progress.cjs",
      "import": "./dist/progress.js"
    },
    "./scroll": {
      "types": "./dist/scroll.d.ts",
      "require": "./dist/scroll.cjs",
      "import": "./dist/scroll.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "build:example": "pnpm run --filter demo... build",
    "test": "vitest --run",
    "test:dev": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "bump": "bumpp --execute gitmoji-changelog --all --commit \"ðŸ”– Release %s\" package.json pnpm-lock.yaml CHANGELOG.md",
    "release": "pnpm run bump && npm publish",
    "start:demo": "pnpm run --filter demo dev",
    "start:docs": "pnpm run --filter documentation start",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run test"
  },
  "peerDependencies": {
    "@react-three/fiber": "^8.8.9",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/three": "^0.145.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.145.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@react-three/fiber": "^8.10.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/three": "^0.144.0",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitest/coverage-c8": "^0.24.5",
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gitmoji-changelog": "^2.3.0",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "three": "^0.145.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "typescript-eslint-language-service": "^5.0.0",
    "vitest": "^0.24.5"
  },
  "dependencies": {
    "immer": "^9.0.17",
    "zustand": "^4.3.2"
  }
}
